<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXSpinner?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox fx:id="box" alignment="CENTER_LEFT" maxHeight="40.0" prefHeight="40.0" prefWidth="200.0" style="-fx-spacing: 10;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <JFXSpinner fx:id="spinner" />
      <ImageView fx:id="icon" fitHeight="27.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/check_icon.png" />
         </image>
      </ImageView>
      <Label fx:id="title" text="Label">
         <font>
            <Font size="15.0" />
         </font></Label>
   </children>
</HBox>
